{
  "consul": "{{ .CONSUL }}:8500",
  "preStart": "python /usr/local/bin/triton-mysql.py",
  "services": [
    {
      "name": "mysql",
      "port": 3306,
      "health": "python /usr/local/bin/triton-mysql.py health",
      "poll": 5,
      "ttl": 25
    }
  ],
  "backends": [
    {
      "name": "mysql-primary",
      "poll": 10,
      "onChange": "python /usr/local/bin/triton-mysql.py on_change"
    }
  ]
}
